import java.util.HashMap;
import java.util.LinkedList;


public class chapter2a {
	
	static void removeDuplicate(LinkedList<Integer> list){
		HashMap<Integer, Integer> map = new HashMap<Integer, Integer>(list.size());
		for (int i=0;i<list.size();i++){
			if(map.get(list.indexOf(i))==null){
				map.put(list.indexOf(i),1);
				System.out.println("i "+i);
			}else{
				list.remove(i);
				System.out.println("remove");
			}
		}
	}
	public static void main(String[] args){
		LinkedList<Integer> mylist = new LinkedList<Integer>();
		
		mylist.add(1);
		mylist.add(6);
		mylist.add(3);
		mylist.add(8);
		mylist.add(2);
		mylist.add(3);
		removeDuplicate(mylist);
		System.out.println(mylist);
	}
	
}


public class chapter5c {
	static void smallBigInt(int num){
		int quotient = num/2;
		int remainder=num%2;
		int numOne =0;
		if(remainder !=0) numOne++;
		while(quotient/2!=0){
			numOne++;
	}
	public static void main(String[] args){
		Double v = 0.3432;
		System.out.println(v.doubleToLongBits(v));
	}
}



public class NodeTree {
	
	int key;
	
	NodeTree left;
	NodeTree right;
	
	NodeTree(){
		
	}
	
	void add(int k){
		NodeTree current=this;
		if (this==null){
			key=k;
		}else{
			if(this.key<k){
				current=this.right;
			}else{
				current=this.left;
			}
		}
	}
	
	public static void main(String[] args){
		NodeTree tree = new NodeTree();
		tree.add(3);
		tree.add(2);
	}
}